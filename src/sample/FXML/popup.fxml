<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="mainWindow" minHeight="0.0" minWidth="0.0" prefHeight="238.0" prefWidth="341.0" stylesheets="@../CSS/Style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ControllerPopup">
   <children>
      <TextField fx:id="name" layoutX="135.0" layoutY="78.0" prefHeight="34.0" prefWidth="162.0" />
      <Button fx:id="playButton" layoutX="190.0" layoutY="156.0" mnemonicParsing="false" onMouseClicked="#launchNewGame" prefHeight="37.0" prefWidth="94.0" styleClass="vert" text="Play">
         <effect>
            <InnerShadow choke="0.16" color="#333e74" height="42.45" radius="15.3625" />
         </effect>
      </Button>
      <Button fx:id="cancelButton" layoutX="48.0" layoutY="158.0" mnemonicParsing="false" onMouseClicked="#closePopup" prefHeight="36.0" prefWidth="92.0" styleClass="record-sales" text="Cancel">
         <effect>
            <InnerShadow blurType="ONE_PASS_BOX" choke="0.27" color="#3d5387" height="56.75" radius="18.9375" />
         </effect>
      </Button>
      <Label fx:id="nameLabel" layoutX="14.0" layoutY="88.0" text="What's your name ?">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label fx:id="newLabel" alignment="CENTER" layoutX="63.0" layoutY="28.0" prefHeight="30.0" prefWidth="210.0" text="Start a whole new adventure">
         <font>
            <Font name="Bauhaus 93" size="15.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
